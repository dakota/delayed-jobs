{
  "name": "uafrica/delayed-jobs",
  "description": "A CakePHP Plugin to manage and process background jobs with priority and retries",
  "type": "cakephp-plugin",
  "require": {
    "cakephp/cakephp": "^3.0",
    "friendsofcake/crud": "^4.0",
    "cakephp/migrations": "^1.5.1",
    "videlalvaro/php-amqplib": "^2.5"
  },
  "require-dev": {
    "cakephp/app": "^3.0",
    "phpunit/phpunit": "^4.5",
    "sizuhiko/fabricate": "~2.0@dev",
    "sizuhiko/cake_fabricate": "dev-develop"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Jaco Roux",
      "email": "jaco@uafrica.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "DelayedJobs\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DelayedJobs\\Test\\": "tests",
      "Cake\\Test\\": "vendor/cakephp/cakephp/tests"
    }
  }
}
